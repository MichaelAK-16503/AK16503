if (username == "Akshay" && password == "darkAK16503") {
document.getElementById("heading").innerHTML = "username is correct";
document.getElementById("welcome").innerHTML = "password is correct";
document.getElementById("heading").style.color = "green";
document.getElementById("welcome").style.color = "green"; } else if (username ==
"Akshay" && password != "darkAK16503") { // alert("Wrong User");
document.getElementById("heading").innerHTML = "username is correct";
document.getElementById("welcome").innerHTML = "password is incorrect";
document.getElementById("heading").style.color = "green";
document.getElementById("welcome").style.color = "red"; } else if (username !=
"Akshay" && password == "darkAK16503") { // alert("Wrong User");
document.getElementById("heading").innerHTML = "username is incorrect";
document.getElementById("welcome").innerHTML = "password is correct";
document.getElementById("heading").style.color = "red";
document.getElementById("welcome").style.color = "green"; } else {
document.getElementById("heading").innerHTML = "username is incorrect";
document.getElementById("welcome").innerHTML = "password is incorrect";
document.getElementById("welcome").style.color = "red";
document.getElementById("heading").style.color = "red"; }
